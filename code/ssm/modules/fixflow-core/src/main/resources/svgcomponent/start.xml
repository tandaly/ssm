	<g id="{svg_id}">
		<g class="stencils" transform="translate({x}, {y})">
			<g class="me">
				<g pointer-events="fill" title="{text}" id="sid-{svg_id}">
					<defs id="sid-{svg_id}-def">
						<radialGradient id="sid-{svg_id}-defbackground" cx="10%" cy="10%" r="100%" fx="10%" fy="10%">
							<stop offset="0%" stop-color="#ffffff" stop-opacity="1" id="sid-{svg_id}-defstop"/>
							<stop id="sid-{svg_id}-deffill_el" offset="100%" stop-color="#ffffff" stop-opacity="1"/>
						</radialGradient>
					</defs>
					<circle id="sid-{svg_id}bg_frame" cx="15" cy="15" r="18" stroke="black" fill="url(#sid-{svg_id}-defbackground) white" stroke-width="1"/>
					<text font-size="12" id="sid-{svg_id}text_name" x="15" y="36" oryx:align="top center" xmlns:oryx="http://www.b3mn.org/oryx" stroke="black" stroke-width="0pt" letter-spacing="-0.01px" oryx:fontSize="11" text-anchor="middle">
						<tspan dy="11" x="15" y="36" id="ext-gen349">{text}</tspan>
					</text>
				</g>
			</g>
			<g class="children" style="overflow:hidden"/>
			<g class="edge"/>
		</g>
		<g class="controls">
			<g class="dockers"/>
			<g class="magnets" transform="translate({x}, {y})">
				<g pointer-events="all" display="none" transform="translate(7, 7)">
					<circle cx="8" cy="8" r="4" stroke="none" fill="red" fill-opacity="0.3"/>
				</g>
			</g>
		</g>
	</g>