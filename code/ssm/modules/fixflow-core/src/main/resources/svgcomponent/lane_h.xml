<g id="{svg_id}">
	<g class="stencils" xmlns:oryx="http://www.b3mn.org/oryx" transform="translate({x}, {y})">
		<g class="me">
			<g pointer-events="none" title="{text}"
				id="sid-{svg_id}">
				<defs
					id="sid-{svg_id}_sid-{svg_id}_3">
					<radialGradient
						id="sid-{svg_id}background" cx="10%"
						cy="10%" r="100%" fx="10%" fy="10%">
						<stop offset="0%" stop-color="#ffffff" stop-opacity="1"
							id="sid-{svg_id}_sid-{svg_id}_18" />
						<stop id="sid-{svg_id}fill_el"
							offset="100%" stop-color="#FFFFCC" stop-opacity="1" />
					</radialGradient>
				</defs>
				<rect id="sid-{svg_id}border"
					oryx:resize="vertical horizontal" x="0" y="0" width="{width}"
					height="{hight}" stroke="black" stroke-width="1"
					fill="url(#sid-{svg_id}background) white"
					pointer-events="none" />
				<text oryx:rotate="270" font-size="12"
					id="sid-{svg_id}text_name"
					oryx:align="middle center" oryx:anchors="left"
					oryx:fittoelem="caption" fill="black" stroke="black"
					stroke-width="0pt" letter-spacing="-0.01px" 
					oryx:fontSize="11" text-anchor="middle">
					<tspan x="{text_x}" y="20" dy="0">{text}</tspan>
				</text>
			</g>
		</g>
		<g class="children" style="overflow:hidden" />
		<g class="edge" />
	</g>
</g>